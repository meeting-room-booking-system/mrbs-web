	  web/tzurl/zoneinfo/Pacific/Port_Moresby.ics[ADD],
	  web/tzurl/zoneinfo/Pacific/Rarotonga.ics[ADD],
	  web/tzurl/zoneinfo/Pacific/Saipan.ics[ADD],
	  web/tzurl/zoneinfo/Pacific/Tahiti.ics[ADD],
